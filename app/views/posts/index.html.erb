<h2>ホーム</h2>

<table>
  <thead>
    <tr>
      <th>タイトル</th>
      <th>ゲーム名</th>
      <th>評価</th>
    </tr>
  </thead>
  <tbody>
    <% @posts.each do |post| %>
      <tr>
        <td><%= link_to post.title, post_path(post) %></td>
        <td><%= post.name %></td>
        <td><%= post.evaluation %></td>
      </tr>
    <% end %>
  </tbody>
</table>